<?xml version='1.0' encoding='utf-8'?>
<widget id="akvelon.cordova.oneDriveDemoApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BackgroundDownload">
        <param name="wp-package" value="BackgroundDownload" />
    </feature>
    <feature name="File">
        <param name="wp-package" value="File" />
    </feature>
    <name>OneDriveDemoApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
